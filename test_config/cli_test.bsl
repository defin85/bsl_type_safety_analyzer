// Тест CLI анализатора BSL

Функция ТестКлиАнализатора(Дубль, Параметр2, Дубль) Экспорт
    // Дублированный параметр - должна быть ошибка BSL010
    
    // Создаем массив
    Массив = Новый Массив();
    Массив.Добавить("Элемент");     // Корректный вызов
    Массив.НесуществующийМетод();   // Ошибка BSL003 (если UnifiedBslIndex работает)
    
    // Необъявленная переменная
    НеобъявленнаяПеременная = 5;    // Ошибка BSL007
    
    Возврат Массив.Количество();
КонецФункции

